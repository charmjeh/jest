{
  "name": "smart-solution",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@trevoreyre/autocomplete-vue": "^2.0.3",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "cropperjs": "^1.5.6",
    "fabric": "^3.3.2",
    "file-saver": "^2.0.2",
    "form-data": "^2.5.1",
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "vue": "^2.6.10",
    "vue-cropperjs": "^4.0.1",
    "vue-i18n": "^8.12.0",
    "vue-lazyload": "^1.3.3",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.1.2",
    "vuejs-paginate": "^2.1.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^3.9.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tui-calendar": "^1.12.3",
    "tui-date-picker": "^4.0.0",
    "vue-head": "^2.1.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.23.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/base",
      "eslint:recommended"
    ],
    "rules": {
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "generator-star-spacing": "off",
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ]
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "parser": "babel-eslint",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
